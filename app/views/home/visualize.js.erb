$(document).ready(function()    {
    $('#result').html("<%=escape_javascript render(:partial => 'visualize') %>");
    var new_position = $('#result').offset();
    if ($('#queryrun').val()){
        $('html, body').stop().animate({ scrollTop: new_position.top }, 500);
        $('#queryrun').val(false);
    }

    console.log("refresh");
});
